{
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.exclude": {
    "node_modules": true,
    "build": true,
  },
  "[typescript][json][jsonc][yaml][github-actions-workflow][markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
  },
  "[lua][luau]": {
    "editor.defaultFormatter": "JohnnyMorganz.stylua",
    "editor.formatOnSave": true,
  },
  "stylua.searchBinaryInPATH": true,
  "luau-lsp.sourcemap.rojoProjectFile": "test.project.json",
  "luau-lsp.types.definitionFiles": [".vscode/definitions/testez.d.luau"],
}
